{
  "name": "data-labelling-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npx prisma generate && node server.js",
    "dev": "nodemon server.js",
    "migrate:actions": "npx prisma migrate dev --name test",
    "migrate:test": "npx prisma migrate dev --name test --schema=./prisma/schema.test.prisma",
    "docker:up": "docker-compose -f tests/docker-compose.yml up -d",
    "docker:down": "docker-compose -f tests/docker-compose.yml down",
    "test:actions": "npm run docker:up && npm run migrate:actions && jest -i --no-cache  --detectOpenHandles --runInBand --forceExit",
    "test:dev": "npm run migrate:test && jest --coverage --no-cache  --detectOpenHandles --runInBand --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.30.3",
    "apollo-server-express": "^3.3.0",
    "argon2": "^0.28.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "googleapis": "^84.0.0",
    "graphql": "^15.5.2",
    "graphql-date": "^1.0.3",
    "graphql-upload": "^12.0.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.12",
    "prisma": "^2.30.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.4",
    "apollo-server": "^3.3.0",
    "apollo-server-integration-testing": "^3.0.0",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "fetch": "^1.1.0",
    "form-data": "^4.0.0",
    "jest": "^27.1.0",
    "node-fetch": "^3.0.0"
  }
}
